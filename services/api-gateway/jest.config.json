{
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
        "server.js",
        "middleware/**/*.js",
        "!node_modules/**",
        "!coverage/**"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 80,
            "functions": 80,
            "lines": 80,
            "statements": 80
        }
    },
    "testTimeout": 10000,
    "maxWorkers": 1
}